{
  "name": "@gyugyu/webfonts-generator",
  "version": "0.3.0",
  "description": "Webfonts generator CLI",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "bin",
    "src",
    "lib",
    "tsconfig.json"
  ],
  "bin": {
    "generate-webfonts": "bin/generate-webfonts.js"
  },
  "scripts": {
    "exec": "ts-node src/command.ts",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib",
    "compile": "tsc",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "font",
    "webfont",
    "svg",
    "ttf",
    "woff",
    "woff2"
  ],
  "author": "Yusuke Yagyu <gyugyu@pepabo.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^6.1.0",
    "glob": "^7.1.6",
    "js-yaml": "^3.14.0",
    "mkdirp": "^1.0.4",
    "svg2ttf": "^5.0.0",
    "svgicons2svgfont": "^9.1.1",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.2",
    "ttf2woff2": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/commander": "^2.12.2",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.14",
    "@types/js-yaml": "^3.12.5",
    "@types/mkdirp": "^1.0.1",
    "@types/svg2ttf": "^5.0.0",
    "@types/ttf2woff2": "^2.0.0",
    "jest": "^26.5.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
