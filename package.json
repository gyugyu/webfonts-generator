{
  "name": "@gyugyu/webfonts-generator",
  "version": "0.3.0",
  "description": "Webfonts generator CLI",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "bin",
    "src",
    "lib",
    "tsconfig.json"
  ],
  "bin": {
    "generate-webfonts": "bin/generate-webfonts.js"
  },
  "scripts": {
    "exec": "ts-node src/command.ts",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib",
    "compile": "tsc",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "font",
    "webfont",
    "svg",
    "ttf",
    "woff",
    "woff2"
  ],
  "author": "Yusuke Yagyu <gyugyu@pepabo.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.2.0",
    "glob": "^8.0.1",
    "js-yaml": "^4.1.0",
    "mkdirp": "^1.0.4",
    "svg2ttf": "^5.2.0",
    "svgicons2svgfont": "^9.2.0",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.2",
    "ttf2woff2": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/commander": "^2.12.2",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/js-yaml": "^4.0.5",
    "@types/mkdirp": "^1.0.2",
    "@types/svg2ttf": "^5.0.1",
    "@types/ttf2woff2": "^2.0.0",
    "jest": "^27.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
